{
  "configurations": [
    {
      "type": "node-terminal",
      "name": "debug: datawarehouse-sync",
      "request": "launch",
      "command": "yarn workspace data-warehouse-sync  serve",
      "cwd": "${workspaceFolder}/"
    },
    {
      "type": "node-terminal",
      "name": "debug: api-gateway",
      "request": "launch",
      "command": "yarn workspace api-gateway serve",
      "cwd": "${workspaceFolder}/"
    },
    {
      "type": "node-terminal",
      "name": "debug: certificates",
      "request": "launch",
      "command": "yarn workspace certificates serve",
      "cwd": "${workspaceFolder}/"
    },
    {
      "type": "node-terminal",
      "name": "debug: frontend",
      "request": "launch",
      "command": "yarn workspace frontend dev",
      "cwd": "${workspaceFolder}/"
    }
  ]
}
